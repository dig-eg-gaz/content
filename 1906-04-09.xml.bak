                            </row>
                            <row>
                                <cell>Caledonia</cell>
                                <cell><date when="1905-07-18">18 July</date></cell>
                                <cell>Arabia</cell>
                                <cell><date when="1905-08-15">15 August</date></cell>
                                <cell>Egypt</cell>
                                <cell><date when="1905-09-12">12 Sept</date>.</cell>
                            </row>
