                            <row>
                                <cell/>
                                <cell>1st</cell>
                                <cell>2nd</cell>
                            </row>
                            <row>
                                <cell>London</cell>
                                <cell>£ 14. 5/</cell>
                                <cell>£ 9</cell>
                            </row>
                            <row>
