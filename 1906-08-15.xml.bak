                                <cell>Persia</cell>
                                <cell><date when="1905-08-29">29 August</date></cell>
                            </row>
                            <row>
                                <cell>Britannia</cell>
                                <cell><date when="1905-07-11">11 July</date></cell>
                                <cell>Marmora</cell>
